<div class="navbar navbar-expand-sm navbar-light navbar-ets fixed-top">
  <%= link_to root_path, class: "navbar-brand" do %>
    <%= image_tag "/menu-bar.svg", id: "menu-bar" %>
  <% end %>

  <div class="link-pages">
    <% if current_user %>
      <%= link_to "Sign Out", class: "link-font", style: "padding-right: 44px;", method: :delete %>
    <% else %>
      <%= link_to "Login", new_user_registration_path, class: "link-font", style: "margin-right: 40px;" %>
    <% end %>

    <%= link_to "Bio / Studio", bio_path, class: "link-font", style: "margin-right: 30px;" %>
    <%= link_to "Keep in touch", contact_path, class: "link-font" %>
  </div>

</div>
